<%= form_for @vendor do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %><br>

	<%= f.label :password %> 
	<%= f.text_field :password %><br>

	<%= f.label :email %> 
	<%= f.text_field :email %> <br>

	<%= f.label :phone %> 
	<%= f.text_field :phone %> <br>


	<%= f.label :comment %> 
	<%= f.text_field :comment %><br>

	<%= f.label :address %> 
	<%= f.text_field :address %><br>


<%= f.submit %>
<% end %>
