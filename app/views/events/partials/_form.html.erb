<%= form_for @event do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %><br>

	<%= f.label :date %> 
	<%= f.text_field :date %> <br>

	<%= f.label :description %> 
	<%= f.text_field :description %> <br>


	<%= f.label :address %> 
	<%= f.text_field :address %><br>

    <%= f.label :city %> 
  <%= f.text_field :city %><br>


<%= f.submit %>
<% end %>
