<%= form_for @event do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %><br>

	<%= f.label :location %> 
	<%= f.text_field :location %> <br>

	<%= f.label :date %> 
	<%= f.text_field :date %> <br>


	<%= f.label :description %> 
	<%= f.text_field :description %><br>

<%= f.submit %>
<% end %>