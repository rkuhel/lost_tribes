<%= render partial: "/events/partials/nav.html.haml" %> 

	<table>
	<% @events.each do |event| %> 
	<tr>
	 <td> <%= link_to event.title, event %> </td>
		<td> <%= event.date %> </td>
		<td> <%= event.location %> </td>
		<td> <%= event.description %> </td>
	<% end %>
	</tr>
	</table>
